{
  "name": "metapress",
  "version": "1.0.0",
  "private": true,
  "author": "dhruvjangid",
  "description": "The Pulse of Creativity",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "prepare": "husky",
    "dev": "turbo dev -F web",
    "build": "turbo build -F web",
    "start": "turbo start -F web",
    "check-types": "turbo check-types",
    "clean": "rm -rf apps/web/.next apps/web/.turbo apps/web/node_modules bun.lock .turbo"
  },
  "dependencies": {},
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.1.6",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "lint-staged": {
    "**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,astro,svelte}": ""
  },
  "packageManager": "bun@1.2.20"
}
